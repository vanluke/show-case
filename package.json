  "name": "show-case",
  "version": "0.0.1",
  "description": "Show case",
  "main": "index.js",
  "scripts": {
    "stats": "webpack --env stats --profile --json > stats.json",
    "start": "npm run start:client && npm run start:server",
    "start:server": "node_modules/.bin/nodemon",
    "start:server2": "node --inspect ./index.js",
    "start:client": "node_modules/.bin/webpack-dev-server --port=3000 --inline --progress --history-api-fallback --open --colors",
    "test": "npm run test:single -- --watch",
    "test:single": "NODE_ENV=test node ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require ./config/test.config.js 'client/**/**/*.test.js' --recursive"
  },
